{
  "name": "gatsby-starter-bonsaicss",
  "description": "Gatsby starter styled with BonsaiCSS",
  "version": "4.0.0",
  "author": "Astrid GÃ¼nther <info@astrid-guenther.de>",
  "dependencies": {
    "bonsai.css": "^1.2.2",
    "gatsby": "^3.5.1",
    "gatsby-plugin-manifest": "^3.5.0",
    "gatsby-plugin-offline": "^4.5.1",
    "gatsby-plugin-react-helmet": "^4.5.0",
    "gatsby-plugin-sharp": "^3.5.0",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0"
  },
  "keywords": [
    "gatsby",
    "bonsai",
    "bonsaicss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "format": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "7.27.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.23.3",
    "eslint-plugin-react": "7.23.2",
    "gatsby-plugin-eslint": "3.0.0",
    "prettier": "2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/astridx/gatsby-starter-bonsaicss"
  },
  "bugs": {
    "url": "https://github.com/astridx/gatsby-starter-bonsaicss/issues"
  }
}
